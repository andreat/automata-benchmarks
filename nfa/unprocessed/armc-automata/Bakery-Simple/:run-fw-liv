N=1

T=100000

RunIt_Good() {
I=1;
while [ $I -le $N ]; do

echo $X
cpp -std=iso9899:1990 $X > \:go.pl
ulimit -t $T
yap -h 50000 -s 50000 -t 50000 << XXX
reconsult(':go'), asmc_time_yap, halt.
XXX

I=$((I+1))

done

}

RunIt_Bad() {
I=1;
while [ $I -le $N ]; do

echo $X
cpp -std=iso9899:1990 $X > \:go.pl
ulimit -t $T
yap -h 50000 -s 50000 -t 50000 << XXX
reconsult(':go'), asmc_bad_time_yap, halt.
XXX

I=$((I+1))

done

}

##################################################################################
#echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

X="-DNoAbs -P asmc_ver_liv.pl"; RunIt_Bad

##################################################################################
#echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

#X="-DIncBad -DIniBad -DFlColl -P asmc_ver_liv.pl"; RunIt_Bad

####################################################################################

#X="-DIncHalfBad -DIniHalfBad -DFlColl -P asmc_ver_liv.pl"; RunIt_Bad

####################################################################################

#X="-DIncOne -DIniOne -DFlColl -P asmc_ver_liv.pl"; RunIt_Bad

