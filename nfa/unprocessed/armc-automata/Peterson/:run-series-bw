N=3

T=1

RunIt_Rev() {
I=1;
while [ $I -le $N ]; do

echo $X
cpp -std=iso9899:1990 $X > \:go.pl
ulimit -t $T
yap -h 250000 -s 250000 -t 250000 << XXX
reconsult(':go'), asmc_rev_time_yap, halt.
XXX

I=$((I+1))

done

}

##################################################################################
echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

X="-DIntrs -DPrBad -DFwColl -P asmc_rev_ver.pl"; RunIt_Rev

echo "-----------------------------------------------"

X="-DIntrsTail -DPrBad -DFwColl -P asmc_rev_ver.pl"; RunIt_Rev

echo "-----------------------------------------------"

X="-DIntrsTailKey -DPrBad -DFwColl -P asmc_rev_ver.pl"; RunIt_Rev

##################################################################################
echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

