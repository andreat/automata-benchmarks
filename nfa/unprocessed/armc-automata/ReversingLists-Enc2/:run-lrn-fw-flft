N=3

T=10

RunIt_LrnFw() {
I=1;
while [ $I -le $N ]; do

echo $X
cpp -std=iso9899:1990 $X > \:go.pl
ulimit -t $T
yap -h 50000 -s 50000 -t 50000 << XXX
reconsult(':go'), lrn_time_verify_yap, halt.
XXX

I=$((I+1))

done

}

##################################################################################
echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

X="-DFlFtColl -DLrnGenConfBad -DLrnIniCollHalfGen -P asmc_lrn.pl"; RunIt_LrnFw

X="-DFlFtColl -DLrnGenConfHalfBad -DLrnIniCollHalfGen -P asmc_lrn.pl"; RunIt_LrnFw

X="-DFlFtColl -DLrnGenConfTwiceBad -DLrnIniCollHalfGen -P asmc_lrn.pl"; RunIt_LrnFw

echo "-----------------------------------------------"

X="-DFlFtColl -DLrnGenConfBad -DLrnIniCollOne -P asmc_lrn.pl"; RunIt_LrnFw

X="-DFlFtColl -DLrnGenConfHalfBad -DLrnIniCollOne -P asmc_lrn.pl"; RunIt_LrnFw

X="-DFlFtColl -DLrnGenConfTwiceBad -DLrnIniCollOne -P asmc_lrn.pl"; RunIt_LrnFw

echo "-----------------------------------------------"

X="-DFlFtColl -DLrnGenConfBad -DLrnIniCollZero -P asmc_lrn.pl"; RunIt_LrnFw

X="-DFlFtColl -DLrnGenConfHalfBad -DLrnIniCollZero -P asmc_lrn.pl"; RunIt_LrnFw

X="-DFlFtColl -DLrnGenConfTwiceBad -DLrnIniCollZero -P asmc_lrn.pl"; RunIt_LrnFw

echo "-----------------------------------------------"

X="-DFlFtColl -DLrnGenConfInit -DLrnIniCollHalfGen -P asmc_lrn.pl"; RunIt_LrnFw

X="-DFlFtColl -DLrnGenConfHalfInit -DLrnIniCollHalfGen -P asmc_lrn.pl"; RunIt_LrnFw

X="-DFlFtColl -DLrnGenConfTwiceInit -DLrnIniCollHalfGen -P asmc_lrn.pl"; RunIt_LrnFw

echo "-----------------------------------------------"

X="-DFlFtColl -DLrnGenConfInit -DLrnIniCollOne -P asmc_lrn.pl"; RunIt_LrnFw

X="-DFlFtColl -DLrnGenConfHalfInit -DLrnIniCollOne -P asmc_lrn.pl"; RunIt_LrnFw

X="-DFlFtColl -DLrnGenConfTwiceInit -DLrnIniCollOne -P asmc_lrn.pl"; RunIt_LrnFw

echo "-----------------------------------------------"

X="-DFlFtColl -DLrnGenConfInit -DLrnIniCollZero -P asmc_lrn.pl"; RunIt_LrnFw

X="-DFlFtColl -DLrnGenConfHalfInit -DLrnIniCollZero -P asmc_lrn.pl"; RunIt_LrnFw

X="-DFlFtColl -DLrnGenConfTwiceInit -DLrnIniCollZero -P asmc_lrn.pl"; RunIt_LrnFw

echo "-----------------------------------------------"

X="-DFlFtColl -DLrnGenConfOne -DLrnIniCollZero -P asmc_lrn.pl"; RunIt_LrnFw

##################################################################################
echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

