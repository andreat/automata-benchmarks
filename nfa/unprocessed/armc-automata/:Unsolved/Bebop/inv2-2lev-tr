fa(

%begin sigma and symbols
t(fsa_preds,fsa_preds),
%end sigma and symbols

15, % number of states

[ % begin start states
0
], % end start states

[ % begin final states
1
], % end final states

[ % begin transitions

%%%%%%% LEVEL 2

trans(0,$@(in(['0','1']))/ $@(in(['0','1'])),3),
trans(3,$@(in([a,b,c,d,e,f]))/ $@(in([a,b,c,d,e,f])),4),
trans(4,'2'/'2',5),
trans(5,$@(in(['0','1']))/ $@(in(['0','1'])),6),
trans(6,$@(in(['0','1']))/ $@(in(['0','1'])),7),
trans(7,$@(in(['0','1']))/ $@(in(['0','1'])),8),
trans(8,$@(in([d,e]))/ $@(in([d,e])),10),

%%%%%%% LEVEL 1

trans(0,$@(in(['0','1']))/ $@(in(['0','1'])),9),
trans(9,$@(in([a,b,c,d,e,f]))/ $@(in([a,b,c,d,e,f])),10),
trans(10,'1'/'1',11),
trans(11,$@(in(['0','1']))/ $@(in(['0','1'])),12),
trans(12,$@(in(['0','1']))/ $@(in(['0','1'])),13),
trans(13,$@(in(['0','1']))/ $@(in(['0','1'])),14),
trans(14,$@(in([d,e]))/ $@(in([d,e])),16),

%%%%%%%  THE MAIN FUNCTION LEVEL

trans(3,$@(in([p,q,r,s,t]))/ $@(in([p,q,r,s,t])),2),
trans(14,$@(in([q,r]))/ $@(in([q,r])),2),
trans(2,$@(in(['0','1']))/ $@(in(['0','1'])),1)

], % end transitions
[]). % jumps
