fa(

%begin sigma and symbols
r(fsa_preds),
%end sigma and symbols

18, % number of states

[ % begin start states
0
], % end start states

[ % begin final states
1
], % end final states

[ % begin transitions

%%%%%%% LEVEL 3

trans(0,in(['0','1']),2),
trans(2,in([a,b,c,d,e,f]),3),
trans(3,'3',4),
trans(4,in(['0','1']),5),
trans(5,in(['0','1']),6),
trans(6,in(['0','1']),7),

%%%%%%% LEVEL 2

trans(0,in(['0','1']),7),
trans(7,in([a,b,c,d,e,f]),8),
trans(8,'2',9),
trans(9,in(['0','1']),10),
trans(10,in(['0','1']),11),
trans(11,in(['0','1']),12),

%%%%%%% LEVEL 1

trans(0,in(['0','1']),12),
trans(12,in([a,b,c,d,e,f]),13),
trans(13,'1',14),
trans(14,in(['0','1']),15),
trans(15,in(['0','1']),16),
trans(16,in(['0','1']),17),

%%%%%%%  THE MAIN FUNCTION LEVEL

trans(2,in([p,q,r,s,t]),1),
trans(17,in([p,q,r,s,t]),1)

], % end transitions
[]). % jumps
