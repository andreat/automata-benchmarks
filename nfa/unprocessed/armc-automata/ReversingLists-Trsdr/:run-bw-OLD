N=1

T=2

RunIt_Rev() {
I=1;
while [ $I -le $N ]; do

echo $X
cpp -std=iso9899:1990 $X > \:go.pl
ulimit -t $T
yap -h 50000 -s 50000 -t 50000 << XXX
reconsult(':go'), asmc_rev_time_yap, halt.
XXX

I=$((I+1))

done

}

##################################################################################
#echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

#X="-DTrClosure -DIntrs -DPrBad -DFwColl -P asmc_rev_ver.pl"; RunIt_Rev

#echo "-----------------------------------------------"

#X="-DTrClosure -DIntrsTail -DPrBad -DFwColl -P asmc_rev_ver.pl"; RunIt_Rev

#echo "-----------------------------------------------"

#X="-DTrClosure -DIntrsTailKey -DPrBad -DFwColl -P asmc_rev_ver.pl"; RunIt_Rev

##################################################################################
#echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

#X="-DTrClosure -DIntrs -DPrBad -DPrGuards -DFwColl -P asmc_rev_ver.pl"; RunIt_Rev

#echo "-----------------------------------------------"

#X="-DTrClosure -DIntrsTail -DPrBad -DPrGuards -DFwColl -P asmc_rev_ver.pl"; RunIt_Rev

#echo "-----------------------------------------------"

#X="-DTrClosure -DIntrsTailKey -DPrBad -DPrGuards -DFwColl -P asmc_rev_ver.pl"; RunIt_Rev

##################################################################################
echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
