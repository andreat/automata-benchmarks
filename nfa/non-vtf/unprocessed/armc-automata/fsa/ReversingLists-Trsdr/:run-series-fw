N=3

T=28

RunIt_Good() {
I=1;
while [ $I -le $N ]; do

echo $X
cpp -std=iso9899:1990 $X > \:go.pl
ulimit -t $T
yap -h 50000 -s 50000 -t 50000 << XXX
reconsult(':go'), asmc_time_yap, halt.
XXX

I=$((I+1))

done

}

RunIt_Bad() {
I=1;
while [ $I -le $N ]; do

echo $X
cpp -std=iso9899:1990 $X > \:go.pl
ulimit -t $T
yap -h 50000 -s 50000 -t 50000 << XXX
reconsult(':go'), asmc_bad_time_yap, halt.
XXX

I=$((I+1))

done

}

##################################################################################
echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

X="-DTrClosure -DIncBad -DIniBad -DFlColl -P asmc_ver.pl"; RunIt_Bad

####################################################################################

X="-DTrClosure -DIncHalfBad -DIniHalfBad -DFlColl -P asmc_ver.pl"; RunIt_Bad

####################################################################################

X="-DTrClosure -DIncOne -DIniOne -DFlColl -P asmc_ver.pl"; RunIt_Bad

##################################################################################
echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

X="-DTrClosure -DIncBad -DIniBad -DFtColl -P asmc_ver.pl"; RunIt_Bad

