N=1

T=3

RunIt_Good() {
I=1;
while [ $I -le $N ]; do

echo $X
cpp -std=iso9899:1990 $X > \:go.pl
ulimit -t $T
yap -h 50000 -s 50000 -t 50000 << XXX
reconsult(':go'), asmc_time_yap, halt.
XXX

I=$((I+1))

done

}

RunIt_Bad() {
I=1;
while [ $I -le $N ]; do

echo $X
cpp -std=iso9899:1990 $X > \:go.pl
ulimit -t $T
yap -h 50000 -s 50000 -t 50000 << XXX
reconsult(':go'), asmc_bad_time_yap, halt.
XXX

I=$((I+1))

done

}

##################################################################################
echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

X="-DIntrs -DPrBad -DFwColl -P asmc_ver.pl"; RunIt_Bad

echo "-----------------------------------------------"

X="-DIntrsTail -DPrBad -DFwColl -P asmc_ver.pl"; RunIt_Bad

echo "-----------------------------------------------"

X="-DIntrsTailKey -DPrBad -DFwColl -P asmc_ver.pl"; RunIt_Bad

##################################################################################
echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

X="-DIntrs -DPrBad -DPrGuards -DFwColl -P asmc_ver.pl"; RunIt_Bad

echo "-----------------------------------------------"

X="-DIntrsTail -DPrBad -DPrGuards -DFwColl -P asmc_ver.pl"; RunIt_Bad

echo "-----------------------------------------------"

X="-DIntrsTailKey -DPrBad -DPrGuards -DFwColl -P asmc_ver.pl"; RunIt_Bad

##################################################################################
echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

X="-DIntrs -DPrIni -DFwColl -P asmc_ver.pl"; RunIt_Bad

echo "-----------------------------------------------"

X="-DIntrsTail -DPrIni -DFwColl -P asmc_ver.pl"; RunIt_Bad

echo "-----------------------------------------------"

X="-DIntrsTailKey -DPrIni -DFwColl -P asmc_ver.pl"; RunIt_Bad


##################################################################################
echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

X="-DIntrs -DPrIni -DPrGuards -DFwColl -P asmc_ver.pl"; RunIt_Bad

echo "-----------------------------------------------"

X="-DIntrsTail -DPrIni -DPrGuards -DFwColl -P asmc_ver.pl"; RunIt_Bad

echo "-----------------------------------------------"

