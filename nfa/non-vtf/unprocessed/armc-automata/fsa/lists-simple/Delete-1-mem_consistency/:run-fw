N=1

T=15

RunIt_Good() {
I=1;
while [ $I -le $N ]; do

echo $X
cpp -std=iso9899:1990 $X > \:go.pl
ulimit -t $T
yap -h 50000 -s 50000 -t 50000 << XXX
reconsult(':go'), asmc_time_yap, halt.
XXX

I=$((I+1))

done

}

RunIt_Bad() {
I=1;
while [ $I -le $N ]; do

echo $X
cpp -std=iso9899:1990 $X > \:go.pl
ulimit -t $T
yap -h 50000 -s 50000 -t 50000 << XXX
reconsult(':go'), asmc_bad_time_yap, halt.
XXX

I=$((I+1))

done

}

##################################################################################
#echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

#X="-DNoAbs -P asmc_ver.pl"; RunIt_Bad

##################################################################################
echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

X="-DIntrs -DPrBad -DFwColl -P asmc_ver.pl"; RunIt_Bad

echo "-----------------------------------------------"

X="-DIntrsTail -DPrBad -DFwColl -P asmc_ver.pl"; RunIt_Bad

echo "-----------------------------------------------"

X="-DIntrsTailKey -DPrBad -DFwColl -P asmc_ver.pl"; RunIt_Bad

