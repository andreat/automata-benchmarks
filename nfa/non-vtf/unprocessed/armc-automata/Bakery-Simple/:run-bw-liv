N=1

T=2

RunIt_Rev() {
I=1;
while [ $I -le $N ]; do

#echo $X
cpp -std=iso9899:1990 $X > \:go.pl
ulimit -t $T
yap -h 50000 -s 50000 -t 50000 << XXX
reconsult(':go'), asmc_rev_time_yap, halt.
XXX

I=$((I+1))

done

}

##################################################################################
echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

X="-DIncBad -DIniBad -DFlColl -P asmc_rev_ver_liv.pl"; RunIt_Rev

####################################################################################

X="-DIncHalfBad -DIniHalfBad -DFlColl -P asmc_rev_ver_liv.pl"; RunIt_Rev

####################################################################################

X="-DIncOne -DIniOne -DFlColl -P asmc_rev_ver_liv.pl"; RunIt_Rev

##################################################################################
echo "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
##################################################################################

