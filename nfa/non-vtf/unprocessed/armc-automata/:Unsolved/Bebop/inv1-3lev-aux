fa(

%begin sigma and symbols
r(fsa_preds),
%end sigma and symbols

20, % number of states

[ % begin start states
0
], % end start states

[ % begin final states
1
], % end final states

[ % begin transitions

%%%%%%% LEVEL 3

trans(0,in(['0','1']),2),
trans(2,in([a,b,c,d,e,f]),3),
trans(3,'3',4),
trans(4,in(['0','1']),5),
trans(5,in(['0','1']),6),
trans(6,in(['0','1']),7),
trans(7,in([d,e]),9),

%%%%%%% LEVEL 2

trans(0,in(['0','1']),8),
trans(8,in([a,b,c,d,e,f]),9),
trans(9,'2',10),
trans(10,in(['0','1']),11),
trans(11,in(['0','1']),12),
trans(12,in(['0','1']),13),
trans(13,in([d,e]),15),

%%%%%%% LEVEL 1

trans(0,in(['0','1']),14),
trans(14,in([a,b,c,d,e,f]),15),
trans(15,'1',16),
trans(16,in(['0','1']),17),
trans(17,in(['0','1']),18),
trans(18,in(['0','1']),19),
trans(19,in([d,e]),21),

%%%%%%%  THE MAIN FUNCTION LEVEL

trans(2,in([p,q,r,s,t]),1),
trans(19,in([q,r]),1)

], % end transitions
[]). % jumps
